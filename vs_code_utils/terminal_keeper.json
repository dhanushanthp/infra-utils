                    ]
                },
                {
                    "name": "Session 2B",
                    "autoExecuteCommands": true,
                    "icon": "desired-icon",
                    "color": "terminal.ansiYellow",
                    "commands": [
                        "command for session 2B"
                    ]
                }
            ]
        ]
    }
